page {
    background-color: #FFF;
    height: 100%;
    font-family: "PingFang SC", HiraginoSansGB-W3, "Microsoft Yahei", "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 14px;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 100 rpx 0;
    box-sizing: border-box;
}

view {
    box-sizing: border-box;
}
